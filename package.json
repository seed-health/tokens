{
  "name": "@seed-health/tokens",
  "version": "1.0.0",
  "description": "Design tokens synced from Figma Variables",
  "main": "build/js/tokens.module.js",
  "module": "build/js/tokens.js",
  "types": "build/js/tokens.d.ts",
  "files": [
    "build/"
  ],
  "scripts": {
    "fetch-tokens": "node scripts/fetch-figma-tokens.js",
    "fetch-styles": "node scripts/fetch-figma-styles.js",
    "fetch-all": "npm run fetch-tokens && npm run fetch-styles",
    "build-tokens": "style-dictionary build --config style-dictionary.config.mjs",
    "sync": "npm run fetch-all && npm run build-tokens",
    "validate": "tz check",
    "validate:build": "npm run build-tokens",
    "test": "npm run validate && npm run validate:build",
    "diff": "node scripts/diff-tokens.js",
    "prepublishOnly": "npm run build-tokens"
  },
  "keywords": [
    "design-tokens",
    "design-system",
    "figma",
    "style-dictionary",
    "tokens"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=20.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seed-health/tokens.git"
  },
  "dependencies": {
    "style-dictionary": "^4.0.1"
  },
  "devDependencies": {
    "@terrazzo/cli": "^0.10.3",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  }
}
